const uint8_t frameCount = sizeof(frames) / sizeof(frames[0]);
const uint16_t frameDelay = 70;  // ms between frames

void setup() {
  Wire.begin();  
  if (!display.begin(SSD1306_SWITCHCAPVCC, SCREEN_ADDRESS)) {
    // OLED init failed
    for (;;);
  }
  display.clearDisplay();
}

void loop() {
  for (uint8_t i = 0; i < frameCount; i++) {
    display.clearDisplay();
    // read pointer to this frame out of PROGMEM
    const uint8_t* bitmap = (const uint8_t*)pgm_read_ptr(&frames[i]);
    display.drawBitmap(
      0, 0,               // start at top-left corner
      bitmap,             // frame data
      SCREEN_WIDTH,
      SCREEN_HEIGHT,
      SSD1306_WHITE
    );
    display.display();
    delay(frameDelay);
  }
}
